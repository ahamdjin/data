/// Run `npx prisma migrate dev --name init` after editing this schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model FhirRecord {
  id        Int      @id @default(autoincrement())
  resource  Json
  createdAt DateTime @default(now())
}
